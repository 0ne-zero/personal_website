@model IEnumerable<Personal_Website.Models.Experience>
@{
    ViewData["Title"] = "Expriences List";
    Layout = "AdminLayout";
}
<br />
<h1 style="color:gray">
    Expriences List
</h1>
<br />
<p>
    <a class="btn btn-success btn-sm" type="button" href="/admin/create?t=experience">Add Exprience</a>
</p>
<table class="table table-dark table-striped">
    <thead>
        <tr>
            <th>Company Name</th>
            <th>Time Period</th>
            <th>Description</th>
            <th>Commnads</th>
        </tr>
    </thead>
    <tbody>
        @foreach (var item in Model)
        {
            <tr>
                <td>
                    @item.Company_Name
                </td>
                <td>@item.Time_Period</td>
                <td>@item.Description</td>
                <td>
                    <a type="button" class="btn btn-danger btn-sm" href="/admin/delete?t=experience&id=@item.Id&callback=/admin/experiences">Delete</a>
                </td>
            </tr>
        }
    </tbody>
</table>